<%= label_tag "group", "Tables", class: "col-sm-3 control-label" %>
<div class="btn-group col-sm-9" role="group">
  <% @tables.each do |table| %>
    <button type="button" class="btn btn-default" onclick="viewTableSeatsStatus(this)" data-table="<%= table.id%>"><%= table.name %></button>
  <% end %>
</div>
